  <docblock>
    <description compact="An authentication adapter for AuthComponent.  Provides the ability to authenticate using COOKIE">{{{
$this-&gt;Auth-&gt;authenticate = array(
	'Authenticate.Cookie' =&gt; array(
		'fields' =&gt; array(
			'username' =&gt; 'username',
			'password' =&gt; 'password'
 		),
		'userModel' =&gt; 'User',
		'scope' =&gt; array('User.active' =&gt; 1),
		'crypt' =&gt; 'rijndael'// Defaults to rijndael(safest), optionally set to 'cipher' if required
	)
)
}}}</description>
  </docblock>
  <extends full="BaseAuthenticate" namespace="" name="BaseAuthenticate"/>
  <method name="authenticate" start="33" end="57" abstract="false" final="false" static="false" visibility="public">
    <docblock>
      <description compact="Authenticates the identity contained in the cookie.  Will use the `settings.userModel`, and `settings.fields` to find COOKIE data that is used to find a matching record in the `settings.userModel`.  Will return false if there is no cookie data, either username or password is missing, of if the scope conditions have not been met."/>
      <param description="The unused request object" type="object" variable="$request">
        <type full="CakeRequest" namespace="" name="CakeRequest"/>
      </param>
      <param description="Unused response object." type="object" variable="$response">
        <type full="CakeResponse" namespace="" name="CakeResponse"/>
      </param>
      <return description="False on login failure.  An array of User data on success." type="object">
        <type full="mixed." namespace="" name="mixed."/>
      </return>
    </docblock>
    <parameter name="request" byreference="false" type="object">
      <type full="\\CakeRequest" namespace="\" name="CakeRequest"/>
    </parameter>
    <parameter name="response" byreference="false" type="object">
      <type full="\\CakeResponse" namespace="\" name="CakeResponse"/>
    </parameter>
  </method>
